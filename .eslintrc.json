{
  "extends": "airbnb-base",
  "env": {
    "es6": true,
    "browser": true,
    "jest": true
  },
  "rules": {
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ],
    "no-plusplus": "off",
    "max-len": "off",
    "no-param-reassign": 0,
    "no-proto": "off"
  }
}